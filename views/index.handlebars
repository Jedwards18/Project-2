<div class="container" id="formContainer">
    <div class="formHFlex">
        <h3 id="form-header">Enter Your Project Information Below</h3>
        <a class="btn tooltipped" data-position="bottom" data-tooltip="Select the project type that most interests you and learn how similar strategies have fared.">?</a>
    </div>
    <div class="card">
    <form id="new-project-form">
        <div class="input-field col s6">
              <input id="project-name" type="text" class="validate" required>
              <label for="project-name">Your Project Name</label>
          </div>
        <div class="row">
            <div class="input-field col s6">
                <select id="category">
                    <option value="" disabled selected>Choose a category</option>
                    <option value="art">Art</option>
                    <option value="comics">Comics</option>
                    <option value="crafts">Crafts</option>
                    <option value="dance">Dance</option>
                    <option value="design">Design</option>
                    <option value="fashion">Fashion</option>
                    <option value="film & video">Film & Video</option>
                    <option value="food">Food</option>
                    <option value="games">Games</option>
                    <option value="journalism">Journalism</option>
                    <option value="music">Music</option>
                    <option value="photography">Photography</option>
                    <option value="publishing">Publishing</option>
                    <option value="technology">Technology</option>
                    <option value="theater">Theater</option>
                </select>
                <label>Category</label>
            </div>
            <div class="input-field col s6">
                <select id="country">
                    <option value="" disabled selected>Choose a country</option>
                    <option value="US">United States</option>
                    <option value="GB">Great Britain</option>
                    <option value="CA">Canada</option>
                </select>
                <label>Country</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s3 offset-s3">
                <input id="min-goal" type="text" class="validate">
                <label for="min-goal">Minimum Goal</label>
            </div>
            <div class="input-field col s3">
                <input id="max-goal" type="text" class="validate">
                <label for="max-goal">Maximum Goal</label> 
            </div>
        </div>
        <div class="row">
            <a id="submit-button" class="waves-effect waves-light btn-large col s2 offset-s4" data-toggle="modal" data-targets="#exampleModal">Submit</a>
           
        </div>
    </form>
    </div>
</div>

<div id="resultsContainer" class="container">  
    <div class="card">
       <div class="row">
          <div class="col s2 offset-s2">
            <h4>Results</h4>
          </div> 
        </div> 
        <div class="row">
          <div class="col s2 offset-s3">
            <div id="piechart" style="width: 700px; height: 390px;"></div> 
          </div> 
        </div> 
        <ul class="collapsible popout">
            <li>
                <div class="collapsible-header">
                    <i class="material-icons">check</i> Successes
                 </div>
                <div class="collapsible-body">
                    <table class="highlight">
                      <thead>
                          <tr>
                              <th>Name</th>
                              <th>Category</th>
                              <th>Country</th>
                              <th>Pledged</th>
                              <th>Goal</th>
                              <th>Backers</th>
                          </tr>
                      </thead>
                      <tbody id="successBody"></tbody> <!--successful results response appends here-->
                    </table>
                </div>
            </li>
            <li>
                <div class="collapsible-header">
                    <i class="material-icons">close</i>Failures
                 </div>
                <div class="collapsible-body">
                    <table class="highlight">
                      <thead>
                          <tr>
                              <th>Name</th>
                              <th>Category</th>
                              <th>Country</th>
                              <th>Pledged</th>
                              <th>Goal</th>
                              <th>Backers</th>
                          </tr>
                      </thead>
                      <tbody id="failureBody"></tbody> <!--failure results response appends here-->
                    </table>
                </div>
            </li>
        </ul>
    </div>
</div>


    
